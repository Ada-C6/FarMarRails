<table>
<tr>
  <th>Market Name</th>
  <th>Address</th>
  <th>City</th>
  <th>County</th>
  <th>State</th>
  <th>Zipcode</th>
  <th colspan="2"></th>
</tr>
    <tr>
      <td><%= @mymarket.name %> </td>
      <td><%= @mymarket.address %></td>
      <td><%= @mymarket.city %></td>
      <td><%= @mymarket.county %></td>
      <td><%= @mymarket.state%></td>
      <td><%= @mymarket.zip %></td>
    </tr>
</table>

<p><%= link_to "See our Vendors!", vendor_show_path(@mymarket[:id]) %><p>

<% @vendors.each do |vendor| %>

  <% if vendor.market_id == @mymarket.id %>
  <%=  vendor.name %>
  <%  end %>

<% end %>
