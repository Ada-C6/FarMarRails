<h1><%= @market.name %></h1>
<p>Market ID:<%= @market.id %>

<h3>Address</h3>
<p>Street: <%= @market.address %></p>
<p> City: <%= @market.city %></p>
<p> County: <%= @market.county %></p>
<p>State: <%= @market.state %></p>
<p>Zip Code: <%= @market.zip %></p>

<h3>Vendors</h3>
<% @vendors_list.each do |vendor|%>
  <p><%= vendor.name %></p>
<% end %>

<%= link_to "Edit Market Info", edit_market_path(@market.id) %>
