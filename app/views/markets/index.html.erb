<div class="container">
<body class="market-view">

<h3>Here's a list of all our current running markets: </h3>

<h4><%= link_to("Add a New Market", new_market_path) %></h4>


<% @markets.each do |market| %>
  <br><%= link_to("#{market.name}", market_path(market.id)) %>
  <b><%= link_to "Edit Market Info", edit_market_path(market.id) %></b>
<% end %>

</body>
</div>
