<h1>Choose Your Market</h1>

<% @markets.each do |market| %>
  <article>
    <p> <%= market.name %>  </p>
    <p> <%= button_to "Edit", {action: "edit", id: market.id}, method: :get %></p>
  </article>
<% end %>
<p>
  <%= button_to "Add Market", {controller: 'markets', action: 'new'}, {path: 'new', method: :get} %>
</p>
