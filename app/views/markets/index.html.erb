<h1>Market#index</h1>
<article>
  <ul>
    <% @markets.each do |market| %>
      <li><%= link_to market.name, {controller: 'markets', action: 'show', id: market.id}, method: :get %></li>
      <li><%= market.address %></li>
      <li><%= market.city %></li>
      <li><%= market.county %></li>
      <li><%= market.state %></li>
      <li><%= market.zip %></li>
    <% end %>
  </ul>
</article>
