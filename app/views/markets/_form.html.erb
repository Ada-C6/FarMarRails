<%= form_for @market, method: http_method do |f| %>
  <br><%= f.label :name, "Market Name" %>
  <%= f.text_field :name %>

  <br><%= f.label :address, "Street Address" %>
  <%= f.text_field :address %>

  <br><%= f.label :city, "City" %>
  <%= f.text_field :city %>

  <br><%= f.label :county, "County" %>
  <%= f.text_field :county %>

  <br><%= f.label :zip, "Zipcode" %>
  <%= f.text_field :zip %>

  <br><%= f.submit "Submit"%>
<% end %>
