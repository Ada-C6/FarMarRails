<%# @TODO - add validations for model attributes & handle errors  %>
<%= form_for @market, method: http_method do |f| %>
<ul>
  <li>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </li>
  <li>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  </li>
  <li>
  <%= f.label :city %>
  <%= f.text_field :city %>
  </li>
  <li>
  <%= f.label :state %>
  <%= f.text_field :state %>
  </li>
  <li>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  </li>
  <%= f.submit %>
</ul>
<% end %>
