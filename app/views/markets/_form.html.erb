<%= form_for @market, method: http_method do |t| %>

<p>
  <label for="ref_id">Reference ID</label>
  <%# t.label :ref_id %>
  <%= t.text_field :ref_id %>
</p>

<p>
  <%= t.label :name %>
  <%= t.text_field :name %>
</p>


<p>
  <%= t.label :address %>
  <%= t.text_field :address %>
</p>

<p>
  <%= t.label :city %>
  <%= t.text_field :city %>
</p>

<p>
  <%= t.label :county %>
  <%= t.text_field :county %>
</p>

<p>
  <%= t.label :state %>
  <%= t.text_field :state %>
</p>

<p>
  <%= t.label :zip %>
  <%= t.text_field :zip %>
</p>

<%= t.submit "Submit" %>

<% end %>
