<h1>Edit your market profile!</h1>
<p>Use the form below to edit your profile. When you are done, press submit!</p>

<section>
  <%= form_for @market, method: 'put', url: markets_update_path(@market.id) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :county %>
    <%= f.text_field :county %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

    <%= f.submit %>
  <% end %>
</section>
