<section class="body">
  <%# render partial: 'form', url: { action: "update" }, method: :PATCH %>
  <%= form_for @market, url: {controller: 'markets', action: 'update'}, method: :patch do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :county %>
    <%= f.text_field :county %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

    <%= f.submit %>
  <% end %>
</section>
