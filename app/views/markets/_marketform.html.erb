<%= @error if @error %>
<%= form_for @mymarket,
  method: @post_method,
  url: @post_path do |f| %>
  <!-- <%= @error if @error %> -->
  <div class="form_attribute">
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
<div class="form_attribute">
  <%= f.label :street %>
  <%= f.text_field :street %>
</div>
<div class="form_attribute">
  <%= f.label :city %>
  <%= f.text_field :city %>
</div>
<div class="form_attribute">
  <%= f.label :county %>
  <%= f.text_field :county %>
</div>
<div class="form_attribute">
  <%= f.label :state %>
  <%= f.text_field :state %>
</div>
<div class="form_attribute">
  <%= f.label :zip %>
  <%= f.text_field :zip %>
</div>
<div class="form_attribute">
  <%= f.submit %>
</div>
  <% end %>
