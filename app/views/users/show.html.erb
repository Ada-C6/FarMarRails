<div class="individual-market">
  <div class="overall-header-text">
    <h1><%= @market.name %></h1>
  </div>
  <h3>Address</h3>
  <br /><%= @market.address %>
  <br /><%= @market.city %>
  <br /><%= @market.state %>
  <br /><%= @market.zip %>
  <br /><%= @market.county %> County

  <h3>Vendor list</h3>
  <ul>
    <li> <%@market.vendors.all.each do |vendor|%>
       <%=vendor.name%>
      <%end%>
    </li>
  </ul>
  <br />

</div>
