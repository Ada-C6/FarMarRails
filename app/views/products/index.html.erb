<h1>Products by Vendor:</h1>
<ol>
  <% @all_products_by_vendor.each do | product | %><li>
    <p>
      <%= link_to "#{product.name}", edit_vendor_product_path(vendor_id: product.vendor_id, id: product.id)%>
    </p>

    <%= button_to "delete", vendor_product_path(vendor_id: product.vendor_id, id: product.id), method: :delete,
                      data: { confirm: "Are you sure you want to delete this task?" } %>
  <% end %></li>
</ol>
