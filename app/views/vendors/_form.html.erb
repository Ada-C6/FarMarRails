<%= form_for(vendor_create_or_update), html: {class: 'form'} do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :no_of_employees %><br>
    <%= f.text_area :no_of_employees %>
  </p>
  <p>
    <%= f.submit :class => 'create_button'  %>
  </p>
<% end %>
