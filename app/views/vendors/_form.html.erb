<%= form_for @vendor do |v| %>
  <p><%= v.label :name %>
  <%= v.text_field :name %></p>

  <p><%= v.label "Reference ID" %>
  <%= v.text_field :ref_id %></p>

  <p><%= v.label :num_employees %>
  <%= v.text_field :num_employees %></p>

  <p><%= v.label "Market ID" %>
  <%= v.text_field :market_id %></p>

  <p>
    <%= v.submit "Submit" %>
  </p>
<% end %>
