<%= render partial: "vendor_header" , locals: { action_name: "get"}%>

<h2>MARKETS:</h2>
<section class="visitor_list">
  <% @markets.each do |market| %>
  <article>
    <p>
      <%= link_to "#{market.name}", market_for_ven_path(market.id) %>
      <%= market.state %>
    </p>
    <% end %>
  </article>
</section>
